<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/bg_color_ffffff" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:id="@android:id/progress"
        android:top="1dp"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp">

        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="8dp"/>
                <solid android:color="@color/bg_color_0d4d97" />
            </shape>
        </scale>

        <!--<clip>
            <shape>
                <solid android:color="@color/bg_color_ff427c" />
                <corners android:radius="8dp" />
            </shape>
        </clip>-->
    </item>
</layer-list>